{
  "expo": {
    "name": "twixbus",
    "slug": "twixbus",
    "scheme": "twixbus",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#662D91",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#662D91"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#662D91"
      },
      "package": "com.twixbus.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
